{
	"name": "deesoft/yii2-gii",
	"description": "Yii2 custom generator for gii",
	"keywords": ["yii2", "gii"],
	"type": "yii2-extension",
	"license": "BSD-3-Clause",
	"support": {
		"issues": "https://github.com/deesoft/yii2-gii/issues",
		"source": "https://github.com/deesoft/yii2-gii"
	},
	"authors": [
		{
			"name": "Misbahul Munir",
			"email": "misbahuldmunir@gmail.com"
		}
	],
	"require": {
		"yiisoft/yii2": ">=2.0.6",
		"yiisoft/yii2-gii": "~2.0"
	},
	"autoload": {
		"psr-4": {
			"dee\\gii\\": ""
		}
	},
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "yiisoft/yii2-composer": true
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
